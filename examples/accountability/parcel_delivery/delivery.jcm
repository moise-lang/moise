mas parcel_delivery {
	
	agent alice {
		roles: truckDriver in g1
	}
	agent bob {
		roles: truckDriver in g1
	}	
	agent pm {
		roles: parcelManager in g1
	}
	agent ww {
		roles: warehouseWorker in g1
	}
	agent rp {
		roles: routePlanner in g1
	}
	agent c {
		roles: carrier in g1
	}

    //workspace default {
	//}

    organisation o1 : delivery-os.xml {
		group g1 : delivery_group {
			debug
		}
	}

	java-path: ., src/env

}
